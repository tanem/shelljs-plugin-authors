{
  "name": "shelljs-plugin-authors",
  "version": "1.0.1",
  "description": "A ShellJS plugin for generating a list of authors.",
  "main": "src/index.js",
  "bin": "bin/shelljs-authors",
  "scripts": {
    "authors": "./bin/shelljs-authors > AUTHORS",
    "example": "node ./example",
    "postversion": "git push && git push --tags && npm publish",
    "release": "npm version -m 'Release v%s'",
    "test": "jest --coverage",
    "version": "npm t && npm run authors && git add ."
  },
  "keywords": [
    "shelljs",
    "plugin",
    "authors"
  ],
  "author": "Tane Morgan (https://github.com/tanem)",
  "license": "MIT",
  "bugs": "https://github.com/tanem/shelljs-plugin-authors/issues",
  "homepage": "https://github.com/tanem/shelljs-plugin-authors",
  "repository": "github:tanem/shelljs-plugin-authors",
  "devDependencies": {
    "codecov": "^3.0.4",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "shelljs": "^0.8.2"
  },
  "peerDependencies": {
    "shelljs": "^0.8.2"
  },
  "files": [
    "bin",
    "src"
  ]
}
