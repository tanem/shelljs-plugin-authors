{
  "name": "shelljs-plugin-authors",
  "version": "2.0.7",
  "description": "A ShellJS plugin for generating a nicely formatted list of authors.",
  "main": "src/index.js",
  "bin": {
    "shelljs-authors": "bin/shelljs-authors"
  },
  "scripts": {
    "authors": "./bin/shelljs-authors > AUTHORS",
    "changelog": "github_changelog_generator --header-label \"# Changelog\" --future-release v$npm_package_version",
    "example": "node ./example",
    "postversion": "git push && git push --tags && npm publish",
    "release": "npm version -m 'Release v%s'",
    "test": "jest --coverage",
    "version": "npm t && npm run changelog && npm run authors && git add ."
  },
  "keywords": [
    "authors",
    "git",
    "plugin",
    "script",
    "shelljs",
    "shelljs-plugin",
    "shortlog"
  ],
  "author": "Tane Morgan (https://github.com/tanem)",
  "license": "MIT",
  "bugs": "https://github.com/tanem/shelljs-plugin-authors/issues",
  "homepage": "https://github.com/tanem/shelljs-plugin-authors",
  "repository": "github:tanem/shelljs-plugin-authors",
  "devDependencies": {
    "codecov": "3.5.0",
    "jest": "24.8.0",
    "prettier": "1.17.1",
    "shelljs": "0.8.3",
    "sinon": "7.3.2"
  },
  "peerDependencies": {
    "shelljs": "^0.8.3"
  },
  "files": [
    "bin",
    "src"
  ],
  "dependencies": {
    "commander": "^2.20.0"
  }
}
